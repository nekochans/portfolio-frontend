    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
